<%= form_for [@show] do |f| %>
	<%= f.label :artist %>
	<%= f.text_field :artist %>
	<br>
	<%= f.label :venue %>
	<%= f.text_field :venue %>
	<br>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<br>
	<%= f.label :description %>
	<%= f.text_field :description %>
	<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>

	<%= f.submit 'Add Concert' %>

	<% end %>
<p><%= link_to("Concerts", shows_path) %></p>